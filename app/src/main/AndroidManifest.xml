<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sony.StudyInBeihang" >
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sony.StudyInBeihang.Welcome"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sony.StudyInBeihang.MainActivity" />
        <activity android:name="com.example.sony.StudyInBeihang.HelpActivity" />
        <activity android:name="com.example.sony.StudyInBeihang.details"></activity>
        <receiver android:name="com.service.AlarmReceiver"></receiver>
        <service android:name="com.service.LongRunningService"></service>

    </application>

</manifest>
